<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="edu.upenn.recorder"
      android:versionCode="1"
      android:versionName="1.0"><application android:icon="@drawable/icon" android:label="@string/app_name" android:name=".QRFApplicationData">
       
        <activity android:name=".QRF" android:noHistory="true" android:label="QRF">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
		<activity android:name=".QRFMain" android:noHistory="true" android:label="main">
		</activity>
		
        <activity android:name=".QRFStudents" android:noHistory="true" android:label="students">
        </activity>		
		
        <activity android:name=".QRFInput" android:noHistory="true" android:label="input">
        </activity>
        
        <activity android:name=".QRFFinish" android:noHistory="true" android:label="finish">
        </activity>
				   
    </application>
    <uses-sdk android:minSdkVersion="7" />
	
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" /> 
    <uses-permission android:name="android.permission.CAMERA"/>
	<uses-permission android:name="android.permission.RECORD_AUDIO" />    
    <uses-feature android:name="android.hardware.camera" />
    
</manifest> 